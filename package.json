{
  "name": "recma-mdx-change-props",
  "version": "1.0.0",
  "description": "This recma plugin changes the 'props' parameter into '_props' in the function '_createMdxContent'; and makes appropriate changes in order to be able to use the expression containing for example {props.foo} in the mdx.",
  "type": "module",
  "exports": "./dist/esm/index.js",
  "main": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run prebuild && tsc",
    "lint": "eslint .",
    "prettier": "prettier --write .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.cjs",
    "test:file": "NODE_OPTIONS=--experimental-vm-modules jest --config ./jest.config.cjs test.spec.ts",
    "prepack": "npm run build",
    "prepublishOnly": "npm test && npm run prettier && npm run lint"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipikuka/recma-mdx-change-props.git"
  },
  "keywords": [
    "unified",
    "estree",
    "esast",
    "mdx",
    "mdxjs",
    "recma",
    "recma-plugin",
    "recma-mdx",
    "change props",
    "recma-mdx-change-props"
  ],
  "author": "ipikuka <talatkuyuk@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/ipikuka/recma-mdx-change-props#readme",
  "bugs": {
    "url": "https://github.com/ipikuka/recma-mdx-change-props/issues"
  },
  "devDependencies": {
    "@mdx-js/mdx": "^3.0.0",
    "@types/dedent": "^0.7.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "dedent": "^0.7.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "prettier-2": "npm:prettier@^2",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3",
    "unified": "^11.0.4"
  },
  "dependencies": {
    "@types/estree": "^1.0.5",
    "estree-util-visit": "^2.0.0"
  },
  "sideEffects": false
}
